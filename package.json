{
  "name": "@init-kz/ui",
  "version": "1.1.4",
  "description": "Library from init.kz",
  "homepage": "https://init.kz/en",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/init-pkg/init-ui.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "./swiper": {
      "types": "./dist/swiper/index.d.ts",
      "import": "./dist/swiper/index.js",
      "require": "./dist/swiper/index.cjs"
    },
    "./phone": {
      "types": "./dist/phone/index.d.ts",
      "import": "./dist/phone/index.js",
      "require": "./dist/phone/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./headless": {
      "types": "./dist/headless/index.d.ts",
      "import": "./dist/headless/index.js",
      "require": "./dist/headless/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "format": "prettier --write .",
    "serve": "http-server -p 8080",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "tscheck": "sh ./config/linting/tscheck.sh",
    "release": "release-it",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "INIT.KZ"
  ],
  "author": "INIT.KZ",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Sauyrbai Imangali",
      "url": "https://github.com/Imangali-Sauyrbay"
    },
    {
      "name": "Vadim S.",
      "url": "https://github.com/init-vadim"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@emotion/css": "^11.13.5",
    "@eslint/eslintrc": "^3.3.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/builder-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "esbuild-css-modules-plugin": "^3.1.5",
    "eslint": "^9.27.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.13",
    "libphonenumber-js": "^1.12.8",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "release-it": "^19.0.2",
    "sass": "^1.89.0",
    "storybook": "^8.6.14",
    "swiper": "^11.2.8",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.33.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7",
    "zod": "^3.25.32"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/init-pkg/init-ui/issues"
  },
  "peerDependencies": {
    "@emotion/css": ">=11.10.0",
    "clsx": ">=2.0.0",
    "libphonenumber-js": ">=1.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-hook-form": ">=7.50.0",
    "swiper": ">=11.0.0",
    "zod": ">=3.20.0"
  },
  "dependencies": {
    "@maskito/core": "^3.8.0",
    "@maskito/kit": "^3.8.0",
    "@maskito/phone": "^3.8.0",
    "@maskito/react": "^3.8.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
